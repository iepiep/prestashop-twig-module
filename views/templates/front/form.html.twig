{% extends 'page.html.twig' %}

{% block content %}
    <h1>{{ 'Request an Appointment'|trans({}, 'Modules.Dimsymfony.Front') }}</h1>

    {{ form_start(form) }}
        {{ form_row(form.lastname) }}
        {{ form_row(form.firstname) }}
        {{ form_row(form.address) }}
        {{ form_row(form.postal_code) }}
        {{ form_row(form.city) }}
        {{ form_row(form.phone) }}
        {{ form_row(form.email) }}
        {{ form_row(form.date_creneau1) }}
        {{ form_row(form.date_creneau2) }}
        <button type="submit">{{ 'Submit'|trans({}, 'Modules.Dimsymfony.Front') }}</button>
    {{ form_end(form) }}
{% endblock %}
